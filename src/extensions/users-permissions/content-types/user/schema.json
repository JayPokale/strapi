{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false,
    "timestamps": true
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "userId": {
      "type": "uid",
      "required": true
    },
    "location": {
      "type": "string"
    },
    "profilePhoto": {
      "type": "string"
    },
    "bio": {
      "type": "string"
    },
    "about": {
      "type": "text"
    },
    "likesCount": {
      "type": "integer",
      "default": 0
    },
    "viewsCount": {
      "type": "integer",
      "default": 0
    },
    "commentsCount": {
      "type": "integer",
      "default": 0
    },
    "savesCount": {
      "type": "integer",
      "required": false,
      "default": 0
    },
    "followersCount": {
      "type": "integer",
      "default": 0
    },
    "liked": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "viewed": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "commented": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "saved": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "author"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "user"
    }
  }
}
